%meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}/
-# compass stylesheets
= stylesheet_link_tag 'compiled/screen', :media => 'screen, projection'
= stylesheet_link_tag 'compiled/print', :media => 'print'
/[if lt IE 8]
  = stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'
= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js"
= javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.5/jquery-ui.min.js"
= javascript_include_tag "application", 'jquery/rails'
= csrf_meta_tag
= yield(:head)

